<layout version="0.1.0">
<!--Default layout, loads most of the pages-->
	<cms_index_index>
	</cms_index_index>
    <default translate="label" module="page">
        <label>All Pages</label>
        <block type="page/html" name="root" output="toHtml" template="page/2columns-left.phtml">

            <block type="page/html_head" name="head" as="head">
                <action method="addJs"><script>prototype/prototype.js</script></action>
				<action method="addJs"><script>lib/ccard.js</script></action>
                <action method="addJs" ifconfig="dev/js/deprecation"><script>prototype/deprecation.js</script></action>
                <action method="addJs"><script>prototype/validation.js</script></action>
                <action method="addJs"><script>scriptaculous/builder.js</script></action>
                <action method="addJs"><script>scriptaculous/effects.js</script></action>
                <action method="addJs"><script>scriptaculous/dragdrop.js</script></action>
                <action method="addJs"><script>scriptaculous/controls.js</script></action>
                <action method="addJs"><script>scriptaculous/slider.js</script></action>
                <action method="addJs"><script>varien/js.js</script></action>
                <action method="addJs"><script>varien/form.js</script></action>
                <action method="addJs"><script>varien/menu.js</script></action>
                <action method="addJs"><script>mage/translate.js</script></action>
                <action method="addJs"><script>mage/cookies.js</script></action>
				<block type="page/js_cookie" name="js_cookies" template="page/js/cookie.phtml"/>
				<action method="addItem"><type>skin_css</type><name>bootstrap/css/bootstrap.css</name></action>
				<action method="addItem"><type>skin_css</type><name>bootstrap/css/bootstrap-responsive.css</name></action>
                <!--<action method="addCss"><stylesheet>css/docs.css</stylesheet></action>				
                <action method="addCss"><stylesheet>css/addons.css</stylesheet></action>
                <action method="addCss"><stylesheet>css/theme.css</stylesheet></action>                               
			    <action method="addCss"><stylesheet>css/menu.css</stylesheet></action>				
				<action method="addCss"><stylesheet>css/colorbox.css</stylesheet></action>
				<action method="addCss"><stylesheet>css/widgets.css</stylesheet></action>-->
				<action method="addItem"><type>skin_css</type><name>css/font-awesome.css</name><params/></action>				
                <action method="addItem"><type>skin_css</type><name>css/ie.css</name><params/><if>IE</if></action>
                <action method="addItem"><type>skin_css</type><name>css/ie8.css</name><params/><if>IE 8</if></action>
                <action method="addCss"><stylesheet>css/print.css</stylesheet><params>media="print"</params></action>
				<!--action method="addCss"><stylesheet>css/yt-responsive.css</stylesheet></action-->
            </block>

			<block type="core/text_list" name="after_body_start" as="after_body_start"/>

            <block type="page/html_notices" name="global_notices" as="global_notices" template="page/html/notices.phtml" />

            <block type="page/html_header" name="header" as="header">
				<block type="searchboxpro/list" name="searchbox" template="sm/searchboxpro/default.phtml" />
				<block type="cartpro/cart_cartpro" name="linkscartpro" template="sm/cartpro/checkout/cart/linkscartpro.phtml">
				</block>
                <block type="page/template_links" name="top.links" as="topLinks"/>				
                <block type="page/switch" name="store_language" as="store_language" template="page/switch/flags.phtml"/>
				<!--<block type="catalog/navigation" name="mobinav" template="catalog/navigation/mobinav.phtml" />-->				
				<block type="directory/currency" name="currency" as="currency" template="directory/currency-top.phtml"/>
                <block type="page/html_wrapper" name="top.container" as="topContainer" translate="label">
                    <label>Page Header</label>
                    <action method="setElementClass"><value>top-container</value></action>
                </block>
				<block type="core/text_list" name="menubottom" as="menubottom" translate="label">
                	<label>Menu bottom</label>
            	</block>
				<block type="core/text_list" name="megaMenu" as="megaMenu" />
                <block type="core/text_list" name="top.menu" as="topMenu" />
				<block type="core/text_list" name="top.mobinav" as="mobiMenu" />
				<block type="core/text_list" name="drop.links" as="dropLinks" />			
				
               
				<block type="cms/block" name="about-us">
					<action method="setBlockId"><block_id>about-us</block_id></action>
				</block>
				<block type="customer/form_register" name="customer_form_register" template="customer/form/register.phtml" />
				<block type="customer/form_login" name="customer_form_mini_login" after="-" template="customer/form/mini.login.phtml" />
            </block>

           	<block type="page/html_breadcrumbs" name="breadcrumbs" as="breadcrumbs"/>
           		<!--<block type="core/text_list" name="megaMenu" as="megaMenu" />
		   		<block type="core/text_list" name="top.menu" as="topMenu" />-->		   		
           <block type="core/text_list" name="left" as="left" translate="label">
                <label>Left Column</label>
            </block>
			<block type="core/text_list" name="pickcategory" as="pickcategory" translate="label">
                <label>pick category</label>
            </block>						
			<block type="core/text_list" name="spotlight" as="spotlight" translate="label">
                <label>spotlight</label>
            </block>                               
            <block type="core/text_list" name="banner" as="banner" translate="label">
                <label>Banner</label>
            </block>            
            <block type="core/text_list" name="bannerleft" as="bannerleft" translate="label">
                <label>Banner Left</label>
            </block>  						           			
			
            <block type="core/text_list" name="slider" as="slider" translate="label">
                <label>SM Slider</label>
            </block>	
			
            <block type="core/text_list" name="right-slideshow" as="right-slideshow" translate="label">
                <label>SM Basicproduct</label>
            </block>
			
            <block type="core/text_list" name="block-contact-map" as="block-contact-map" translate="label">
                <label>Contact Map</label>
            </block>		   		  
			
			<block type="core/messages" name="global_messages" as="global_messages"/>
            <block type="core/messages" name="messages" as="messages"/>
			
            <block type="core/text_list" name="content" as="content" translate="label">
                <label>Main Content Area</label>
            </block>			
			 <block type="core/text_list" name="content-top" as="content-top" translate="label">
                <label>Main Content Area Top</label>
            </block>
			 <block type="core/text_list" name="slider-home1" as="slider-home1" translate="label">
                <label>Slider home 1</label>
            </block>
			 <block type="core/text_list" name="slider-home2" as="slider-home2" translate="label">
                <label>Slider home 2</label>
            </block>
			<block type="core/text_list" name="content-bottom" as="content-bottom" translate="label">
                <label>Main Content Area Bottom</label>
            </block>
			<block type="core/text_list" name="content-bottom-col1" as="content-bottom-col1" translate="label">
                <label>Main Content Area Bottom 1</label>
            </block>
			<block type="core/text_list" name="content-bottom-col2" as="content-bottom-col2" translate="label">
                <label>Main Content Area Bottom 2</label>
            </block>
			<block type="core/text_list" name="content-bottom-col3" as="content-bottom-col3" translate="label">
                <label>Main Content Area Bottom 3</label>
            </block>
			<block type="core/text_list" name="content-bottom2" as="content-bottom2" translate="label">
                <label>Slogan Bottom</label>
            </block>            
            
			<block type="page/html_footer" name="footer" as="footer" template="page/html/footer.phtml">
			</block>

            <block type="core/text_list" name="before_body_end" as="before_body_end"/>
        </block>	
					
		<reference name="menubottom">			
			<block type="cms/block" name="menu-bottom">
				<action method="setBlockId"><block_id>menu-bottom</block_id></action>
			</block>
        </reference>
		<!-- Mega Menu -->
		<reference name="megaMenu">        
			<block type="megamenu/list" name="megamenu.list.theme">			
			</block>
		</reference>		
		
        <!--Display category-block compare on left-->        
		 <reference name="left">		 			 
			<block type="catalog/navigation" name="catalog.leftcat" template="catalog/navigation/sidenav.phtml" before="-"/>
			<block type="catalog/product_compare_sidebar" name="catalog.compare.sidebar" after="catalog.leftcat" template="catalog/product/compare/sidebar.phtml"/>
        </reference>
			
		<reference name="bannerleft">
			<block type="cms/block" name="banner-left">
				<action method="setBlockId"><block_id>banner-left</block_id></action>
			</block>
		</reference>
		
		<!--<reference name="pickcategory">
			<block type="cms/block" name="pick-category" >
				<action method="setBlockId"><block_id>pick-category</block_id></action>
			</block>
        </reference>-->					
		
        <reference name="right">
        </reference>


		<block type="core/profiler" output="toHtml" name="core_profiler"/>
				
		<!-- Content bottom -->		
		<reference name="content-bottom-col1">
			<block type="cms/block" name="need-to-know">
				<action method="setBlockId"><block_id>need-to-know</block_id></action>
			</block>
			<block type="cms/block" name="store-chain">
				<action method="setBlockId"><block_id>store-chain</block_id></action>
			</block>			
		</reference>
		<reference name="content-bottom-col2">
			<block type="cms/block" name="history">
				<action method="setBlockId"><block_id>history</block_id></action>
			</block>					
		</reference>
		<reference name="content-bottom-col3">
			<block type="cms/block" name="contact-us-bottom">				
				<action method="setBlockId"><block_id>contact-us-bottom</block_id></action>
			</block>
			<block type="cms/block" name="newsletter">
				<action method="setBlockId"><block_id>newsletter</block_id></action>				
			</block>					
		</reference>
		
		<reference name="content-bottom2">
			<block type="cms/block" name="slogan-bottom">
				<action method="setBlockId"><block_id>slogan-bottom</block_id></action>
			</block>					
		</reference>
		<!--END Content bottom -->

		<!-- Bottom Spotlight - spotlight -->
		<reference name="spotlight">
			<block type="cms/block" name="customer-support">
				<action method="setBlockId"><block_id>customer-support</block_id></action>
			</block>
			<block type="cms/block" name="learn-more">
				<action method="setBlockId"><block_id>learn-more</block_id></action>
			</block>
			<block type="cms/block" name="company">
				<action method="setBlockId"><block_id>company</block_id></action>
			</block>
			<block type="cms/block" name="credit-card">
				<action method="setBlockId"><block_id>credit-card</block_id></action>
			</block>
		</reference>
		<!--END Bottom Spotlight - spotlight -->
    </default>

    <print translate="label" module="page">
        <label>All Pages (Print Version)</label>
        <!-- Mage_Page -->
        <block type="page/html" name="root" output="toHtml" template="page/print.phtml">

            <block type="page/html_head" name="head" as="head">
                <action method="addJs"><script>prototype/prototype.js</script></action>
                <action method="addJs"><script>mage/translate.js</script></action>
                <action method="addJs"><script>prototype/validation.js</script></action>
                <action method="addJs"><script>varien/js.js</script></action>
				<action method="addJs"><script>lib/ccard.js</script></action>	
                <action method="addCss"><stylesheet>css/addons.css</stylesheet></action>
                <action method="addCss"><stylesheet>css/layout.css</stylesheet></action>
                <action method="addCss"><stylesheet>css/theme.css</stylesheet></action>

                <action method="addItem"><type>skin_css</type><name>css/ie.css</name><params/><if>IE</if></action>
                <action method="addItem"><type>skin_css</type><name>css/ie7minus.css</name><params/><if>lt IE 7</if></action>

                <action method="addItem"><type>js</type><name>lib/ds-sleight.js</name><params/><if>lt IE 7</if></action>
                <action method="addItem"><type>js</type><name>varien/iehover-fix.js</name><params/><if>lt IE 7</if></action>

                <action method="addCss"><stylesheet>css/print.css</stylesheet><params>media="print"</params></action>
            </block>

            <block type="core/text_list" name="content" as="content" translate="label">
                <label>Main Content Area</label>
            </block>

        </block>
    </print> <!-- Custom page layout handles -->
    <page_empty translate="label">
        <label>All Empty Layout Pages</label>
        <reference name="root">
            <action method="setTemplate"><template>page/empty.phtml</template></action>
            <!-- Mark root page block that template is applied -->
            <action method="setIsHandle"><applied>1</applied></action>
        </reference>
    </page_empty>

    <page_one_column translate="label">
        <label>All One-Column Layout Pages</label>
        <reference name="root">
            <action method="setTemplate"><template>page/1column.phtml</template></action>
            <!-- Mark root page block that template is applied -->
            <action method="setIsHandle"><applied>1</applied></action>
        </reference>
    </page_one_column>

    <page_two_columns_left translate="label">
        <label>All Two-Column Layout Pages (Left Column)</label>
        <reference name="root">
            <action method="setTemplate"><template>page/2columns-left.phtml</template></action>
            <!-- Mark root page block that template is applied -->
            <action method="setIsHandle"><applied>1</applied></action>
        </reference>
    </page_two_columns_left>

    <page_two_columns_right translate="label">
        <label>All Two-Column Layout Pages (Right Column)</label>
        <reference name="root">
            <action method="setTemplate"><template>page/2columns-right.phtml</template></action>
            <!-- Mark root page block that template is applied -->
            <action method="setIsHandle"><applied>1</applied></action>
        </reference>
    </page_two_columns_right>

    <page_three_columns translate="label">
        <label>All Three-Column Layout Pages</label>
        <reference name="root">
            <action method="setTemplate"><template>page/3columns.phtml</template></action>
            <!-- Mark root page block that template is applied -->
            <action method="setIsHandle"><applied>1</applied></action>
        </reference>
    </page_three_columns>
</layout>
